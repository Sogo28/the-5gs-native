<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config>
        <domain includeSubdomains="true">192.168.1.6</domain>
        <trust-anchors>
            <certificates src="@raw/server_crt" />
        </trust-anchors>
    </domain-config>

    <base-config cleartextTrafficPermitted="false">
        <trust-anchors>
            <certificates src="@raw/server_crt" />
        </trust-anchors>
    </base-config>
</network-security-config>